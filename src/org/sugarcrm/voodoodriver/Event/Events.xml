   <!-- HTML Events

        These events correspond directly to HTML elements on the page.
        The ordering of HTML events corresponds to the order they
        appear on W3C's HTML4 page: http://www.w3.org/TR/html4.
   -->

   <!-- ALERT is an orphan event.  It corresponds to a javascript
        action, so it isn't an HTML event, but it's a property of the
        page, so it isn't a VooDooDriver event.  It's most sensible
        just to stick it here.
   -->
   <alert>
      <actions>
         <accessor type="boolean">alert</accessor>
         <accessor type="string">assert</accessor>
         <accessor type="string">assertnot</accessor>
         <accessor type="boolean">exist</accessor>
         <accessor type="boolean">required</accessor>
      </actions>
   </alert>

